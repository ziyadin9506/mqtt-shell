# MQTT Broker Configuration
MQTT_BROKER_URL=tcp://broker.hivemq.com:1883
MQTT_USERNAME=
MQTT_PASSWORD=

# Topic Configuration (change this to something unique!)
MQTT_TOPIC_PREFIX=mqtt-shell-your-unique-id

# Client ID (optional - auto-generated if not set)
MQTT_CLIENT_ID=

# Security - CRITICAL: Change this to a strong password!
# Generate with: openssl rand -base64 32
EXEC_KEY=ChangeThisToAStrongPassword123!

# TLS Configuration (optional)
MQTT_USE_TLS=false
MQTT_CA_FILE=/path/to/ca.crt